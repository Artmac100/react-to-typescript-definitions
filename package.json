{
  "name": "react-to-typescript-definitions",
  "version": "0.3.2",
  "description": "Create typescript definitions files (d.ts) from react components",
  "main": "index.js",
  "bin": {
    "react2dts": "cli.js"
  },
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "mocha -r babel-register 'tests/**/*-test.js'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "repository": {
    "type": "git",
    "url": "KnisterPeter/react-to-typescript-definitions"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.3.17",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.3.13",
    "chai": "3.4.1",
    "cz-conventional-changelog": "1.1.5",
    "in-publish": "2.0.0",
    "mocha": "2.3.4",
    "react": "0.14.3",
    "tslint": "3.2.1",
    "typescript": "1.7.5"
  },
  "dependencies": {
    "babylon": "6.3.21",
    "minimist": "1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
